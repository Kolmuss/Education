<HBox prefHeight="568.0" prefWidth="669.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.AuthController">
   <children>
      <ImageView fitHeight="568.0" fitWidth="217.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Media/main.jpg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="483.0" prefWidth="450.0" style="-fx-background-color: white;">
         <children>
            <ImageView fitHeight="227.0" fitWidth="232.0" layoutX="112.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Media/fashion-logos.jpg" />
               </image>
            </ImageView>
            <TextField fx:id="usernameField" layoutX="121.0" layoutY="249.0" prefHeight="36.0" prefWidth="208.0" promptText="username" style="-fx-background-color: white; -fx-border-color: black; -fx-background-radius: 5px; -fx-border-radius: 3px;">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
            </TextField>
            <TextField fx:id="passwordVisible" layoutX="121.0" layoutY="306.0" prefHeight="36.0" prefWidth="208.0" promptText="password" style="-fx-background-color: white; -fx-border-color: black; -fx-background-radius: 5px; -fx-border-radius: 3px;" visible="false">
               <font>
                  <Font name="Yu Gothic Medium" size="18.0" />
               </font>
            </TextField>
            <PasswordField fx:id="passwordInvisible" layoutX="121.0" layoutY="306.0" prefHeight="36.0" prefWidth="208.0" promptText="password" style="-fx-background-color: white; -fx-border-color: black; -fx-background-radius: 5px; -fx-border-radius: 3px;">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
            </PasswordField>
            <CheckBox fx:id="showPassword" layoutX="173.0" layoutY="353.0" mnemonicParsing="false" text="show password">
               <font>
                  <Font name="Impact" size="13.0" />
               </font>
            </CheckBox>
            <Button fx:id="loginButton" layoutX="113.0" layoutY="395.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="90.0" style="-fx-background-color: black; -fx-border-color: black;" text="Log in" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Impact" size="12.0" />
               </font>
            </Button>
            <Button fx:id="signUpButton" layoutX="249.0" layoutY="395.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="90.0" style="-fx-background-color: white; -fx-border-color: black;" text="Sign up" textAlignment="CENTER">
